<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<class name="com.vehicle.domain.City" table="t_city">
		<id name="adId" column="ad_id">
			<generator class="identity" />
		</id>
		<property name="adSiteId" column="ad_site_id" />
		<property name="adSiteIndex" column="ad_site_index" />
		<property name="sufName" column="suf_name" />
		<property name="md5"/>
	</class>
</hibernate-mapping>
