<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<class name="com.vehicle.domain.App" table="t_app">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		<property name="type" column="type" />
		<property name="name" column="name" />
		<property name="size" column="size"/>
		<property name="short_des" column="short_des"/>
		<property name="long_des" column="long_des"/>
		<property name="poster_num" column="poster_num"/>
		<property name="download_num" column="download_num"/>
	</class>
</hibernate-mapping>

