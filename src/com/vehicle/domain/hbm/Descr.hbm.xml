<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<class name="com.vehicle.domain.Descr" table="t_descr">
		<id name="descrId" column="descr_id">
			<generator class="identity" />
		</id>
		<property name="descrText" column="descr_text" />
		<property name="descrName" column="descr_name" />
		<property name="descrPinyin" column="descr_pinyin" />
        <property name="topicId" column="topic_id"/>
        <property name="boardId" column="board_id"/>
	</class>
</hibernate-mapping>
